<?xml version="1.0" encoding="utf-8"?>
<alsa>
  <client name="Midi Through">
    <port id="0" />
  </client>
  <client name="Midi Fighter Twister"> <!-- Dirty MFT Signal -->
    <port id="0">
      <connection client="Pure Data" port="1" />
    </port>
  </client>
  <client name="QuNexus">
    <port id="0">
      <connection client="amsynth" port="0" />
      <connection client="mio" port="0" />
    </port>
    <port id="1" />
    <port id="2" />
  </client>
  <client name="mio"> <!-- Octatrack. DIN world. -->
    <port id="0">
      <connection client="Pure Data" port="0" />
      <connection client="amsynth" port="0" />
    </port>
  </client>
  <client name="Pure Data">
    <!--
      Pure Data translates weird values from MIDI Fighter into useable values.
      Leaving Defaults on MFT so we get Full Duplex support
    -->
    <port id="2">
      <connection client="amsynth" port="0" />
      <connection client="mio" port="0" />
    </port>
    <port id="3">    
      <connection client="Midi Fighter Twister" port="0" /> <!-- Full Duplex right hurr -->
    </port>
  </client>
  <client name="amsynth">
    <!--
      amSynth outputs values out to the MFT (through pd) on bank/program change
    -->
    <port id="1">
      <connection client="Pure Data" port="0" />
    </port>
  </client>
</alsa>
<jack>
  <client name="system">
    <port name="capture_1">
      <connection port="gx_head_amp:in_0" />
    </port>
    <port name="capture_2" />
  </client>
  <client name="gx_head_amp">
    <port name="out_0">
      <connection port="gx_head_fx:in_0" />
    </port>
  </client>
  <client name="gx_head_fx">
    <port name="out_0">
      <connection port="system:playback_1" />
    </port>
    <port name="out_1">
      <connection port="system:playback_2" />
    </port>
  </client>
</jack>

